package com.java8Features.lambdaEx;

import java.util.ArrayList;
import java.util.List;

public class DataGet {
	
	
	
	
	
	public static List<Book> list()
	{
		
		List<Book>book=new  ArrayList<>();
		Book b1=new Book();
		b1.setAuthor("raghu");
		b1.setBookName("radha");
		b1.setCost("Rs/-");
		b1.setId(3);
		Book b2=new Book();
		b2.setAuthor("raj");
		b2.setBookName("lakshman");
		b2.setCost("pounds");
		b2.setId(4);
		
		book.add(b2);
		book.add(b1);
		
		Book b3=new Book(5, "ghj", "ggg", "rrr");
		book.add(b3);
		return book;
	}

}
