package com.java8Features.lambdaEx;


import java.util.*;


public class StreamMethodEx {

	public static void main(String[] args) {

		
		      List<Integer> asList = Arrays.asList(1,2,5,8,9,10);
		      
		      asList.stream().filter(n -> n%2==0).collect(Collections.toList());
		      
		
		 
	}

}
